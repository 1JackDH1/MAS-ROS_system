<launch>

	<!-- <arg name="model" default="$(env TURTLEBOT3_MODEL)" doc="model type [burger, waffle, waffle_pi]"/>	For using other Turtlebot3 models -->
	
	<group ns="robot1">
		<param name="robot_description" command="$(find xacro)/xacro --inorder $(find turtlebot3_description)/urdf/turtlebot3_burger.urdf.xacro" />
		<param name="tf_prefix" value="robot1_tf" />
		<node pkg="gazebo_ros" type="spawn_model" name="spawn_urdf" args="-urdf -model turtlebot3_burger1 -x 0.0 -y 0.0 -z 0.0 -param robot_description" />
	</group>
	
	<group ns="robot2">
		<param name="robot_description" command="$(find xacro)/xacro --inorder $(find turtlebot3_description)/urdf/turtlebot3_burger.urdf.xacro" />
		<param name="tf_prefix" value="robot2_tf" />
		<node pkg="gazebo_ros" type="spawn_model" name="spawn_urdf" args="-urdf -model turtlebot3_burger2 -x 1.0 -y 0.0 -z 0.0 -param robot_description" />
	</group>

</launch>
